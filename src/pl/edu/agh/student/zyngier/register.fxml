<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<?import javafx.scene.control.Label?>
<GridPane alignment="center" hgap="10" prefHeight="400.0" prefWidth="800.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.student.zyngier.RegisterController">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" prefWidth="151.0" textAlignment="CENTER" textFill="RED" textOverrun="CLIP" />
      <TextField fx:id="emailField"  GridPane.rowIndex="1" />
      <TextField fx:id="passwordField" GridPane.rowIndex="2" />
      <TextField fx:id="firstNameField" GridPane.rowIndex="3" />
      <TextField fx:id="lastNameField" GridPane.rowIndex="4" />
      <TextField fx:id="peselField" GridPane.rowIndex="5" />
      <TextField fx:id="adressField" GridPane.rowIndex="6" />
      <TextField fx:id="zipCodeField" GridPane.rowIndex="7" />
      <Button mnemonicParsing="false" onAction="#registerButton" prefHeight="25.0" prefWidth="151.0" text="Register" GridPane.rowIndex="8" />
      <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="151.0" text="Button" GridPane.rowIndex="9" />
   </children>
</GridPane>
